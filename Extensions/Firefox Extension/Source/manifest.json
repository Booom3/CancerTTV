{
	"applications": {
		"gecko": {
			"id": "CancerTTV@twitch.tv"
		}
	},
	"manifest_version": 2,
	"name": "CancerTTV",
	"version": "1.0",
	
	"description": "Makes Twitch just a little bit worse for everyone else.",
	
	"author": "Booom3",
	"content_scripts": [
		{
			"matches": ["http://www.twitch.tv/*", "https://www.twitch.tv/*"],
			"js": ["Injector.js"],
			"run_at": "document_end"
		}
	],
	"web_accessible_resources": [
		"CancerTTV.js"
	]
}